<!-- Skin: Site.page -->
<skin key="Site.page" lastupdate="2015-05-08T13:17:43.289Z" crc="732898838">
<content>
&lt;!DOCTYPE html&gt;
&lt;html lang="de-DE" ng-app="f5SkinApp" ng-controller="F5SkinCtrl" class="no-js"&gt;
  &lt;head&gt;
    &lt;meta charset="UTF-8"&gt;
    &lt;title&gt;&lt;% site.title %&gt; &lt;% site.tagline %&gt;&lt;/title&gt;
    &lt;meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"&gt;
    &lt;meta name="description" content="{{param.site.description}}"&gt;
    &lt;meta name="viewport" content="width=device-width, initial-scale=1.0"&gt;
    &lt;meta name="author" content="{{param.site.author}}"&gt; 
    &lt;% site.metaRobots %&gt;
    &lt;% site.metaAlternate %&gt;
    &lt;meta property="og:url" content="&lt;% site.href %&gt;"&gt;
    &lt;meta property="og:title" content="{{param.site.title}}"&gt;
    &lt;meta property="og:site_name" content="{{param.site.description}}"&gt;
    &lt;meta property="og:image" content="{{param.site.imgFolder}}og-image.png"&gt;
    &lt;meta property="og:image:type" content="image/png"&gt;
    &lt;meta name="apple-mobile-web-app-capable" content="yes"&gt;
    &lt;meta name="apple-mobile-web-app-status-bar-style" content="black"&gt;
    &lt;link rel="apple-touch-icon-precomposed" sizes="152x152" href="{{param.site.imgFolder}}apple-touch-icon-152x152.png"&gt;
    &lt;link rel="apple-touch-icon-precomposed" sizes="114x114" href="{{param.site.imgFolder}}apple-touch-icon-114x114.png"&gt;
    &lt;link rel="apple-touch-icon-precomposed" sizes="72x72" href="{{param.site.imgFolder}}apple-touch-icon-72x72.png"&gt;
    &lt;link rel="apple-touch-icon-precomposed" sizes="57x57" href="{{param.site.imgFolder}}apple-touch-icon-57x57.png"&gt;
    &lt;link rel="shortcut icon" href="&lt;% image name="favicon" fallback="/favicon" class="icon" as="url" %&gt;"&gt;
    &lt;link rel="stylesheet" href="&lt;% site.url %&gt;main.css"&gt;
    &lt;link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css"&gt;
    &lt;script src="http://static.twoday.net/cdn/files/modernizr-custom-min-js.js"&gt;&lt;/script&gt;
    &lt;script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js"&gt;&lt;/script&gt;
    &lt;script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular.min.js"&gt;&lt;/script&gt;
    &lt;script src="https://googledrive.com/host/0B87rILW4RVIJdzJiMW5WRUdfalE/f5skin2day/head.js"&gt;&lt;/script&gt;&lt;% site.skin name="skinStyles" %&gt;&lt;% site.skin name="coreInfo" %&gt;
  &lt;/head&gt;
  &lt;body ng-class="[param.format.body.class]" ng-style="param.format.body.style"&gt;
    &lt;div id="loader-wrapper"&gt;
      &lt;div id="loader" ng-cloak ng-if="param.site.loader.animation"&gt;&lt;/div&gt;
    &lt;/div&gt;
    &lt;div id="f5wrap" data-offcanvas class="off-canvas-wrap"&gt;
      &lt;div class="inner-wrap"&gt;
        &lt;div ng-class="{'fixed':param.topbar.class.fixed, 'containToGrid':param.topbar.class.containToGrid, 'sticky':param.topbar.class.sticky}"&gt;
          &lt;top-bar id="f5Topbar" ng-cloak role="navigation" back-text="Eine Ebene höher"&gt;
            &lt;ul class="title-area"&gt;
              &lt;li class="name"&gt;
                &lt;h1 ng-hide="param.topbar.title.length===0"&gt;&lt;a href="#"&gt;{{param.topbar.title}}&lt;/a&gt;&lt;/h1&gt;
              &lt;/li&gt;
              &lt;li toggle-top-bar class="menu-icon"&gt;&lt;a href="#"&gt;&lt;span&gt;&lt;/span&gt;&lt;/a&gt;&lt;/li&gt;
            &lt;/ul&gt;
            &lt;top-bar-section&gt;
              &lt;ul id="f5TopbarLeft" class="left"&gt;
                &lt;li id="toolbarTwoday"&gt;&lt;a href="http://twoday.net/"&gt;&lt;i class="fa {{param.topbar.menuItems.twoday.icon}}"&gt;&lt;/i&gt;{{param.topbar.menuItems.twoday.text}}&lt;/a&gt;&lt;/li&gt;
                &lt;li ng-hide="!param.topbar.menuItems.twoday.divider" class="divider"&gt;&lt;/li&gt;
                &lt;li id="toolbarTopics" has-dropdown ng-if="param.topbar.menuItems.topics.drop"&gt;&lt;a href="&lt;% site.url %&gt;"&gt;&lt;i class="fa {{param.topbar.menuItems.topics.icon}}"&gt;&lt;/i&gt;{{param.topbar.menuItems.topics.text}}&lt;/a&gt;&lt;% topiclist prefix="&lt;ul class='dropdown'&gt;" itemprefix="&lt;li&gt;" itemsuffix="&lt;/li&gt;" suffix="&lt;/ul&gt;" %&gt;
                &lt;/li&gt;
                &lt;li id="toolbarTopics" ng-if="!param.topbar.menuItems.topics.drop"&gt;&lt;a href="&lt;% site.url %&gt;"&gt;&lt;i class="fa {{param.topbar.menuItems.topics.icon}}"&gt;&lt;/i&gt;{{param.topbar.menuItems.topics.text}}&lt;/a&gt;&lt;% topiclist prefix="&lt;ul class='dropdown'&gt;" itemprefix="&lt;li&gt;" itemsuffix="&lt;/li&gt;" suffix="&lt;/ul&gt;" %&gt;
                &lt;/li&gt;
                &lt;li ng-hide="!param.topbar.menuItems.topics.divider" class="divider"&gt;&lt;/li&gt;
                &lt;li id="toolbarImages" has-dropdown ng-hide="param.topbar.menuItems.images.items.length===0"&gt;&lt;a href="&lt;% site.url %&gt;images/topics"&gt;&lt;i class="fa {{param.topbar.menuItems.images.icon}}"&gt;&lt;/i&gt;{{param.topbar.menuItems.images.text}}&lt;/a&gt;
                  &lt;ul top-bar-dropdown&gt;
                    &lt;li ng-repeat="item in param.topbar.menuItems.images.items"&gt;&lt;a href="{{item.href}}" target="_blank"&gt;{{item.text}}&lt;/a&gt;&lt;/li&gt;
                  &lt;/ul&gt;
                &lt;/li&gt;
                &lt;li ng-hide="!param.topbar.menuItems.images.divider || param.topbar.menuItems.images.items.length===0" class="divider"&gt;&lt;/li&gt;
                &lt;li id="toolbarContributor" has-dropdown ng-hide="!param.isContributor()"&gt;&lt;a href="&lt;% site.url %&gt;contribute"&gt;&lt;i class="fa {{param.topbar.menuItems.contribute.icon}}"&gt;&lt;/i&gt;{{param.topbar.menuItems.contribute.text}}&lt;/a&gt;
                  &lt;ul top-bar-dropdown&gt;
                    &lt;li&gt;&lt;a href="&lt;% site.url %&gt;stories/create"&gt;beitrag verfassen&lt;/a&gt;&lt;/li&gt;
                    &lt;li&gt;&lt;a href="&lt;% site.url %&gt;stories/main"&gt;beiträge&lt;/a&gt;&lt;/li&gt;
                    &lt;li&gt;&lt;a href="&lt;% site.url %&gt;images/main"&gt;bilder&lt;/a&gt;&lt;/li&gt;
                    &lt;li&gt;&lt;a href="&lt;% site.url %&gt;files/main"&gt;dateien&lt;/a&gt;&lt;/li&gt;
                    &lt;li&gt;&lt;a href="&lt;% site.url %&gt;polls/main"&gt;umfragen&lt;/a&gt;&lt;/li&gt;
                  &lt;/ul&gt;
                &lt;/li&gt;
                &lt;li ng-hide="!param.topbar.menuItems.contribute.divider" class="divider"&gt;&lt;/li&gt;
                &lt;li id="toolbarAdmin" has-dropdown ng-hide="!param.isAdmin()"&gt;&lt;a href="&lt;% site.url %&gt;manage"&gt;&lt;i class="fa {{param.topbar.menuItems.admin.icon}}"&gt;&lt;/i&gt;{{param.topbar.menuItems.admin.text}}&lt;/a&gt;
                  &lt;ul top-bar-dropdown&gt;
                    &lt;li&gt;&lt;a href="&lt;% site.url %&gt;manage"&gt;weblog&lt;/a&gt;&lt;/li&gt;
                    &lt;li&gt;&lt;a href="&lt;% layout.url %&gt;main"&gt;layout&lt;/a&gt;&lt;/li&gt;
                    &lt;li&gt;&lt;a href="&lt;% site.url %&gt;members/main"&gt;mitglieder&lt;/a&gt;&lt;/li&gt;
                    &lt;li&gt;&lt;a href="&lt;% site.url %&gt;modules/sidebar"&gt;module&lt;/a&gt;&lt;/li&gt;
                    &lt;li&gt;&lt;a href="&lt;% site.url %&gt;tdyBillingOverview"&gt;bezahlung&lt;/a&gt;&lt;/li&gt;
                  &lt;/ul&gt;
                &lt;/li&gt;
                &lt;li ng-hide="!param.topbar.menuItems.admin.divider" class="divider"&gt;&lt;/li&gt;
                &lt;li id="toolbarCustom" has-dropdown ng-hide="!param.topbar.addCustomMenu.activate"&gt;&lt;a href=""&gt;&lt;i class="fa {{param.topbar.addCustomMenu.icon}}"&gt;&lt;/i&gt;{{param.topbar.addCustomMenu.text}}&lt;/a&gt;
                  &lt;ul top-bar-dropdown&gt;
                    &lt;li ng-repeat="item in param.topbar.addCustomMenu.items"&gt;
                      &lt;label ng-if="!item.link"&gt;{{item.text}}&lt;/label&gt;&lt;a ng-if="item.link" href="{{item.href}}" target="_blank"&gt;{{item.text}}&lt;/a&gt;
                    &lt;/li&gt;
                  &lt;/ul&gt;
                &lt;/li&gt;
                &lt;li ng-hide="!param.topbar.addCustomMenu.divider" class="divider"&gt;&lt;/li&gt;
              &lt;/ul&gt;
              &lt;ul id="f5TopbarRight" class="right"&gt;
                &lt;li id="toolbarLogin" ng-hide="param.isLoggedIn()"&gt;&lt;a href="&lt;% site.url %&gt;members/login"&gt;&lt;i class="fa {{param.topbar.menuItems.login.icon}}"&gt;&lt;/i&gt;{{param.topbar.menuItems.login.text}}&lt;/a&gt;&lt;/li&gt;
                &lt;li ng-hide="!param.topbar.menuItems.login.divider" class="divider"&gt;&lt;/li&gt;
                &lt;li id="toolbarAccount" has-dropdown ng-hide="!param.isLoggedIn()"&gt;&lt;a href="&lt;% site.url %&gt;members/account"&gt;&lt;i class="fa {{param.topbar.menuItems.account.icon}}"&gt;&lt;/i&gt;{{param.topbar.menuItems.account.text}}&lt;/a&gt;
                  &lt;ul top-bar-dropdown&gt;
                    &lt;li&gt;&lt;a href="&lt;% username as='url' %&gt;"&gt;home&lt;/a&gt;&lt;/li&gt;
                    &lt;li&gt;&lt;a href="&lt;% site.url %&gt;members/edit"&gt;profil&lt;/a&gt;&lt;/li&gt;
                    &lt;li&gt;&lt;a href="&lt;% site.url %&gt;members/logout"&gt;abmelden&lt;/a&gt;&lt;/li&gt;
                  &lt;/ul&gt;
                &lt;/li&gt;
                &lt;li ng-hide="!param.topbar.menuItems.account.divider" class="divider"&gt;&lt;/li&gt;
                &lt;li id="toolbarAbo" has-dropdown ng-hide="!param.isLoggedIn()"&gt;&lt;a href="&lt;% site.url %&gt;members/updated"&gt;&lt;i class="fa {{param.topbar.menuItems.abo.icon}}"&gt;&lt;/i&gt;{{param.topbar.menuItems.abo.text}}&lt;/a&gt;
                  &lt;ul top-bar-dropdown&gt;
                    &lt;li ng-repeat="item in param.topbar.menuItems.abo.items"&gt;&lt;a href="{{item.href}}" target="_blank"&gt;{{item.text}}&lt;/a&gt;&lt;/li&gt;
                  &lt;/ul&gt;
                &lt;/li&gt;
                &lt;li ng-hide="!param.topbar.menuItems.abo.divider" class="divider"&gt;&lt;/li&gt;
                &lt;li id="toolbarMail" ng-hide="!param.topbar.mailIcon.activate"&gt;&lt;a href="" title="{{param.topbar.mailIcon.title}}" ng-click="param.sendMail()"&gt;&lt;i class="fa {{param.topbar.mailIcon.icon}}"&gt;&lt;/i&gt;{{param.topbar.mailIcon.text}}&lt;/a&gt;&lt;/li&gt;
                &lt;li ng-hide="!param.topbar.mailIcon.divider" class="divider"&gt;&lt;/li&gt;
                &lt;li id="toolbarSearch" ng-hide="!param.topbar.searchIcon.activate"&gt;&lt;a href="{{param.topbar.searchIcon.href}}" title="{{param.topbar.searchIcon.title}}"&gt;&lt;i class="fa {{param.topbar.searchIcon.icon}}"&gt;&lt;/i&gt;{{param.topbar.searchIcon.text}}&lt;/a&gt;&lt;/li&gt;
                &lt;li ng-hide="!param.topbar.searchIcon.divider" class="divider"&gt;&lt;/li&gt;
                &lt;li id="toolbarCustomize" ng-hide="!param.topbar.customizeIcon.activate || !param.isAdmin()"&gt;&lt;a href="" ng-click="customize.open()" title="{{param.topbar.customizeIcon.title}}"&gt;&lt;i class="fa {{param.topbar.customizeIcon.icon}}"&gt;&lt;/i&gt;{{param.topbar.customizeIcon.text}}&lt;/a&gt;&lt;/li&gt;
                &lt;li ng-hide="!param.topbar.customizeIcon.divider" class="divider"&gt;&lt;/li&gt;
              &lt;/ul&gt;
            &lt;/top-bar-section&gt;
          &lt;/top-bar&gt;
        &lt;/div&gt;
        &lt;div id="header" ng-cloak ng-class="[param.format.header.class]" ng-style="param.format.header.style"&gt;
          &lt;h1 ng-class="[param.format.title.class]" ng-style="param.format.title.style"&gt;{{param.site.title}}{{param.format.header.titledelimiter}}&lt;small ng-class="[param.format.subtitle.class]" ng-style="param.format.subtitle.style"&gt;{{param.site.subtitle}}&lt;/small&gt;&lt;/h1&gt;
        &lt;/div&gt;
        &lt;div id="f5container" ng-cloak class="container"&gt;
          &lt;div class="row"&gt;
            &lt;div id="{{param.leftColumn.id}}" ng-hide="!param.leftColumn.activate" ng-class="param.leftColumn.class" ng-style="param.leftColumn.style"&gt;&lt;/div&gt;
            &lt;div id="{{param.middleColumn.id}}" ng-hide="!param.middleColumn.activate" ng-class="param.middleColumn.class" ng-style="param.middleColumn.style"&gt;
              &lt;% response.message prefix="&lt;alert ng-hide='msgClose' class='message radius' close='msgClose=true'&gt;" suffix="&lt;/alert&gt;" %&gt;
              &lt;% response.body %&gt;
            &lt;/div&gt;
            &lt;div id="{{param.rightColumn.id}}" ng-hide="!param.rightColumn.activate" ng-class="param.rightColumn.class" ng-style="param.rightColumn.style"&gt;&lt;% site.sidebar01 %&gt;&lt;% site.sidebar02 %&gt;&lt;/div&gt;
          &lt;/div&gt;
        &lt;/div&gt;&lt;a href="#" class="exit-off-canvas"&gt;&lt;/a&gt;
      &lt;/div&gt;
    &lt;/div&gt; 
    &lt;% site.skin name="stdPreferences" %&gt;
    &lt;div id="usrPreferences" style="display:none"&gt;&lt;% site.skin name="usrPreferences" %&gt;&lt;/div&gt; 
    &lt;% site.skin name="genMenuItems" %&gt;
    &lt;% site.skin name="ngTemplates" %&gt;
    &lt;script src="https://googledrive.com/host/0B87rILW4RVIJdzJiMW5WRUdfalE/f5skin2day/app.js"&gt;&lt;/script&gt;
  &lt;/body&gt;
&lt;/html&gt;
</content>
</skin>