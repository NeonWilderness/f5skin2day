                            &lt;/li&gt;
                            &lt;li&gt;
                              &lt;button href="" title="Nach unten" ng-click="menuCustom.movedown($index)" ng-disabled="$index===customize.param.topbar.addCustomMenu.items.length-1" class="tiny radius secondary square"&gt;&lt;i class="fa fa-caret-down fa-fw"&gt;&lt;/i&gt;&lt;/button&gt;
                            &lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/td&gt;
                      &lt;/tr&gt;
                      &lt;tr ng-show="menuCustom.isEditMode || menuCustom.isCreateMode" class="bgwhite"&gt;
                        &lt;td class="vtop"&gt;
                          &lt;label&gt;&amp;nbsp;&lt;/label&gt;
                          &lt;ul class="button-group twinbuttons"&gt;
                            &lt;li&gt;
                              &lt;button href="" title="Speichern" ng-click="menuCustom.save()" class="tiny radius secondary square right5"&gt;&lt;i class="fa fa-check fa-fw"&gt;&lt;/i&gt;&lt;/button&gt;
                            &lt;/li&gt;
                            &lt;li&gt;
                              &lt;button href="" title="Abbrechen" ng-click="menuCustom.cancel()" class="tiny radius secondary square"&gt;&lt;i class="fa fa-times fa-fw"&gt;&lt;/i&gt;&lt;/button&gt;
                            &lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/td&gt;
                        &lt;td class="vtop text-center"&gt;
                          &lt;label&gt;Typ&lt;/label&gt;
                          &lt;div class="switch round"&gt;
                            &lt;input type="checkbox" id="chkCustomType" ng-model="menuCustom.entry.link"/&gt;
                            &lt;label for="chkCustomType"&gt;&lt;/label&gt;
                          &lt;/div&gt;
                        &lt;/td&gt;
                        &lt;td colspan="2" class="vtop"&gt;
                          &lt;div class="bottom15"&gt;
                            &lt;label&gt;{{menuCustom.getEntryType()}}&lt;/label&gt;
                            &lt;input type="text" ng-model="menuCustom.entry.text"/&gt;
                          &lt;/div&gt;
                          &lt;div ng-if="menuCustom.entry.link" class="bottom15"&gt;
                            &lt;label&gt;Linkadresse&lt;/label&gt;
                            &lt;input type="text" ng-model="menuCustom.entry.href"/&gt;
                          &lt;/div&gt;
                        &lt;/td&gt;
                      &lt;/tr&gt;
                      &lt;tr ng-hide="menuCustom.isEditMode || menuCustom.isCreateMode"&gt;
                        &lt;td colspan="4"&gt;
                          &lt;button href="" ng-click="menuCustom.create()" ng-disabled="menuCustom.isEditMode || menuCustom.isCreateMode" class="tiny radius secondary"&gt;&lt;i class="fa fa-plus fa-margin"&gt;&lt;/i&gt;Neuer Schnellmenüeintrag&lt;/button&gt;
                        &lt;/td&gt;
                      &lt;/tr&gt;
                    &lt;/tbody&gt;
                  &lt;/table&gt;
                &lt;/div&gt;
              &lt;/div&gt;
            &lt;/form&gt;
          &lt;/tab&gt;
          &lt;tab&gt;
            &lt;tab-heading&gt;&lt;i class="fa fa-envelope fa-fw fa-margin"&gt;&lt;/i&gt;Spezialmenü&lt;/tab-heading&gt;
            &lt;form ng-controller="MenuSpecialItemsCtrl as menuSpecial"&gt;
              &lt;div ng-hide="menuSpecial.isEditMode || menuSpecial.isCreateMode" class="row collapse"&gt;
                &lt;div class="small-12 columns"&gt;
                  &lt;table id="specialItems"&gt;
                    &lt;thead&gt;
                      &lt;tr&gt;
                        &lt;th&gt;&amp;nbsp;&lt;/th&gt;
                        &lt;th&gt;Menüicon&lt;/th&gt;
                        &lt;th class="text-center"&gt;Aktiviert?&lt;/th&gt;
                        &lt;th&gt;Vorschau&lt;/th&gt;
                        &lt;th&gt;Tooltip / Adresse&lt;/th&gt;
                        &lt;th&gt;Sortieren&lt;/th&gt;
                      &lt;/tr&gt;
                    &lt;/thead&gt;
                    &lt;tbody&gt;
                      &lt;tr ng-repeat="icon in menuSpecial.sortedIcons"&gt;
                        &lt;td&gt;
                          &lt;ul class="button-group twinbuttons"&gt;
                            &lt;li&gt;
                              &lt;button href="" title="Eintrag bearbeiten" ng-click="menuSpecial.edit($index)" class="tiny radius secondary square right5"&gt;&lt;i class="fa fa-edit fa-fw"&gt;&lt;/i&gt;&lt;/button&gt;
                            &lt;/li&gt;
                            &lt;li&gt;
                              &lt;button href="" title="Eintrag entfernen" ng-click="menuSpecial.remove($index)" ng-if="icon.delete" class="tiny radius secondary square"&gt;&lt;i class="fa fa-trash-o fa-fw"&gt;&lt;/i&gt;&lt;/button&gt;
                            &lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/td&gt;
                        &lt;td&gt;&lt;em&gt;{{icon.key}}&lt;/em&gt;&lt;/td&gt;
                        &lt;td class="text-center"&gt;
                          &lt;div class="switch round"&gt;
                            &lt;input type="checkbox" ng-model="icon.activate" id="act{{icon.key}}"/&gt;
                            &lt;label for="act{{icon.key}}"&gt;&lt;/label&gt;
                          &lt;/div&gt;
                        &lt;/td&gt;
                        &lt;td&gt;
                          &lt;top-bar&gt;
                            &lt;top-bar-section&gt;
                              &lt;ul class="left"&gt;
                                &lt;li&gt;&lt;a href="" title="icon.title"&gt;&lt;i class="fa {{icon.icon}}"&gt;&lt;/i&gt; {{icon.text}}&lt;/a&gt;&lt;/li&gt;
                                &lt;li ng-show="icon.divider" class="divider"&gt;&lt;/li&gt;
                              &lt;/ul&gt;
                            &lt;/top-bar-section&gt;
                          &lt;/top-bar&gt;
                        &lt;/td&gt;
                        &lt;td&gt;
                          &lt;div class="specialtitle"&gt;{{icon.title}}&lt;/div&gt;
                          &lt;div class="specialhref"&gt;{{icon.href}}&lt;/div&gt;
                        &lt;/td&gt;
                        &lt;td&gt;
                          &lt;ul class="button-group twinbuttons"&gt;
                            &lt;li&gt;
                              &lt;button href="" title="Nach oben" ng-click="menuSpecial.moveup($index)" ng-disabled="$index===0" class="tiny radius secondary square right5"&gt;&lt;i class="fa fa-caret-up fa-fw"&gt;&lt;/i&gt;&lt;/button&gt;
                            &lt;/li&gt;
                            &lt;li&gt;
                              &lt;button href="" title="Nach unten" ng-click="menuSpecial.movedown($index)" ng-disabled="$index===menuSpecial.sortedIcons.length-1" class="tiny radius secondary square"&gt;&lt;i class="fa fa-caret-down fa-fw"&gt;&lt;/i&gt;&lt;/button&gt;
                            &lt;/li&gt;
                          &lt;/ul&gt;
                        &lt;/td&gt;
                      &lt;/tr&gt;
                      &lt;tr&gt;
                        &lt;td colspan="6"&gt;
                          &lt;button href="" ng-click="menuSpecial.create()" class="tiny radius secondary"&gt;&lt;i class="fa fa-plus fa-margin"&gt;&lt;/i&gt;Neues Menüicon hinzufügen&lt;/button&gt;
                        &lt;/td&gt;
                      &lt;/tr&gt;
                    &lt;/tbody&gt;
                  &lt;/table&gt;
                &lt;/div&gt;
              &lt;/div&gt;
              &lt;div ng-show="menuSpecial.isEditMode || menuSpecial.isCreateMode" class="row collapse"&gt;
                &lt;div class="small-12 columns"&gt;
                  &lt;table class="fullsize headerEditArea"&gt;
                    &lt;thead&gt;
                      &lt;tr&gt;
                        &lt;th&gt;&lt;span ng-if="menuSpecial.isEditMode"&gt;Menüicon&amp;ensp;&lt;span class="blue"&gt;{{menuSpecial.icon.key}}&lt;/span&gt;&amp;ensp;bearbeiten&lt;/span&gt;&lt;span ng-if="menuSpecial.isCreateMode"&gt;Neues Menüicon hinzufügen&lt;/span&gt;&lt;/th&gt;
                      &lt;/tr&gt;
                    &lt;/thead&gt;
                  &lt;/table&gt;
                  &lt;div class="row"&gt;
                    &lt;div class="small-2 columns"&gt;
                      &lt;label&gt;&amp;nbsp;&lt;/label&gt;
                      &lt;ul class="button-group twinbuttons"&gt;
                        &lt;li&gt;
                          &lt;button href="" title="Speichern" ng-click="menuSpecial.save()" class="tiny radius secondary square right5"&gt;&lt;i class="fa fa-check fa-fw"&gt;&lt;/i&gt;&lt;/button&gt;
                        &lt;/li&gt;
                        &lt;li&gt;
                          &lt;button href="" title="Abbrechen" ng-click="menuSpecial.cancel()" class="tiny radius secondary square"&gt;&lt;i class="fa fa-times fa-fw"&gt;&lt;/i&gt;&lt;/button&gt;
                        &lt;/li&gt;
                      &lt;/ul&gt;
                    &lt;/div&gt;
                    &lt;div class="small-10 columns"&gt;
                      &lt;div class="row"&gt;
                        &lt;div class="small-6 columns"&gt;
                          &lt;label&gt;Iconbezeichnung
                            &lt;input type="text" ng-model="menuSpecial.icon.key" ng-disabled="menuSpecial.isKeyLocked()"/&gt;
                          &lt;/label&gt;
                        &lt;/div&gt;
                        &lt;div class="small-3 columns text-center"&gt;
                          &lt;label&gt;Aktiviert?&lt;/label&gt;
                          &lt;div class="switch round"&gt;
                            &lt;input type="checkbox" ng-model="menuSpecial.icon.activate" id="ac{{menuSpecial.icon.key}}"/&gt;
                            &lt;label for="ac{{menuSpecial.icon.key}}"&gt;&lt;/label&gt;
                          &lt;/div&gt;
                        &lt;/div&gt;
                        &lt;div class="small-3 columns text-center"&gt;
                          &lt;label&gt;Trenner?&lt;/label&gt;
                          &lt;div class="switch round"&gt;
                            &lt;input type="checkbox" ng-model="menuSpecial.icon.divider" id="dv{{menuSpecial.icon.key}}"/&gt;
                            &lt;label for="dv{{menuSpecial.icon.key}}"&gt;&lt;/label&gt;
                          &lt;/div&gt;
                        &lt;/div&gt;
                      &lt;/div&gt;
                      &lt;div class="row"&gt;
                        &lt;div class="small-6 columns"&gt;
                          &lt;label&gt;Text
                            &lt;input type="text" ng-model="menuSpecial.icon.text"/&gt;
                          &lt;/label&gt;
                        &lt;/div&gt;
                        &lt;div class="small-6 columns"&gt;
                          &lt;label&gt;Tooltip
                            &lt;input type="text" ng-model="menuSpecial.icon.title"/&gt;
                          &lt;/label&gt;
                        &lt;/div&gt;
                      &lt;/div&gt;
                      &lt;div class="row"&gt;
                        &lt;div class="small-12 columns"&gt;
                          &lt;label ng-if="menuSpecial.icon.key==='mail'"&gt;E-Mail-Adresse (wird verschlüsselt gespeichert)
                            &lt;input type="email" ng-model="menuSpecial.icon.href"/&gt;
                          &lt;/label&gt;
                          &lt;label ng-if="menuSpecial.icon.key!=='mail' &amp;amp;&amp;amp; !menuSpecial.isUrlLocked()"&gt;URL
                            &lt;input type="url" ng-model="menuSpecial.icon.href"/&gt;
                          &lt;/label&gt;
                        &lt;/div&gt;
                      &lt;/div&gt;
                      &lt;div class="row"&gt;
                        &lt;div class="small-12 columns"&gt;
                          &lt;div class="row collapse"&gt;
                            &lt;label&gt;Font Awesome Iconname/n&lt;a href="http://fortawesome.github.io/Font-Awesome/cheatsheet/" target="_blank" title="Font Awesome Icons anzeigen" class="extlink"&gt;&lt;/a&gt;&lt;/label&gt;
                            &lt;div class="small-10 columns"&gt;
                              &lt;input type="text" ng-model="menuSpecial.icon.icon"/&gt;
                            &lt;/div&gt;
                            &lt;div class="small-2 columns"&gt;&lt;span class="postfix"&gt;&lt;i ng-class="menuSpecial.getMenuIcon(menuSpecial.icon.icon)"&gt;&lt;/i&gt;&lt;/span&gt;&lt;/div&gt;
                          &lt;/div&gt;
                        &lt;/div&gt;
                      &lt;/div&gt;
                      &lt;div class="row"&gt;
                        &lt;div class="small-6 columns"&gt;
                          &lt;label&gt;Vorschau&lt;/label&gt;
                          &lt;top-bar&gt;
                            &lt;top-bar-section&gt;
                              &lt;ul class="left"&gt;
                                &lt;li&gt;&lt;a href="" title="{{menuSpecial.icon.title}}" ng-class="menuSpecial.getPreviewClass()" ng-show="menuSpecial.icon.activate"&gt;
                                    &lt;i class="fa {{menuSpecial.icon.icon}}"&gt;&lt;/i&gt;
                                    {{menuSpecial.icon.text}}&lt;/a&gt;&lt;/li&gt;
                                &lt;li ng-show="menuSpecial.icon.divider &amp;amp;&amp;amp; menuSpecial.icon.activate" class="divider"&gt;&lt;/li&gt;
                              &lt;/ul&gt;
                            &lt;/top-bar-section&gt;
                          &lt;/top-bar&gt;
                        &lt;/div&gt;
                        &lt;div class="small-3 columns text-center"&gt;
                          &lt;label&gt;Iconfarbe&lt;/label&gt;
                          &lt;spectrum-colorpicker ng-model="menuSpecial.icon.color"&gt;&lt;/spectrum-colorpicker&gt;
                        &lt;/div&gt;
                        &lt;div class="small-3 columns text-center"&gt;
                          &lt;label&gt;Hover&lt;/label&gt;
                          &lt;spectrum-colorpicker ng-model="menuSpecial.icon.hover"&gt;&lt;/spectrum-colorpicker&gt;
                        &lt;/div&gt;
                      &lt;/div&gt;
                    &lt;/div&gt;
                  &lt;/div&gt;
                &lt;/div&gt;
              &lt;/div&gt;
            &lt;/form&gt;
          &lt;/tab&gt;
        &lt;/tabset&gt;
      &lt;/tab&gt;
      &lt;tab heading="Aktualisierung"&gt;
        &lt;form ng-controller="CustAppUpdateCtrl as appupdate"&gt;
          &lt;div class="row"&gt;
            &lt;div class="small-12 columns bottom15"&gt;Ihre derzeitige Layout-Version ist &lt;strong&gt;{{customize.param.update.version}}&lt;/strong&gt;.&lt;/div&gt;
          &lt;/div&gt;
          &lt;div class="row"&gt;
            &lt;div class="small-4 columns"&gt;
              &lt;label&gt;Aktualisierungsprüfung&lt;/label&gt;&lt;a class="button tiny split"&gt;{{customize.param.update.check}} &lt;span dropdown-toggle="#dropdown-updatecheck"&gt;&lt;/span&gt;&lt;/a&gt;
              &lt;ul id="dropdown-updatecheck" class="f-dropdown"&gt;
                &lt;li ng-repeat="frequency in appupdate.updateChecks"&gt;&lt;a ng-click="appupdate.useFrequency($index)"&gt;{{frequency.check}}&lt;/a&gt;&lt;/li&gt;
              &lt;/ul&gt;
            &lt;/div&gt;
            &lt;div class="small-4 columns"&gt;
              &lt;label&gt;Datum der letzten Prüfung&lt;/label&gt;
              &lt;div id="dtLastCheck"&gt;{{customize.param.update.lastCheck | date: 'dd.MM.yyyy HH:mm'}}&lt;/div&gt;
            &lt;/div&gt;
            &lt;div class="small-4 columns"&gt;
              &lt;label&gt;Datum der nächsten Prüfung&lt;/label&gt;
              &lt;div id="dtNextCheck"&gt;{{appupdate.dateNextCheck()}}&lt;/div&gt;
            &lt;/div&gt;
          &lt;/div&gt;
          &lt;div class="row"&gt;
            &lt;div class="small-12 columns"&gt;&lt;a href="" ng-show="appupdate.msgClose" ng-click="appupdate.checkReleaseUpdate($event)" class="button small radius secondary"&gt;&lt;i class="fa fa-refresh fa-margin" ng-class="{'fa-spin':appupdate.isChecking}"&gt;&lt;/i&gt;Jetzt auf mögliche Aktualisierung prüfen...&lt;/a&gt;
              &lt;alert ng-if="appupdate.releaseLoaded &amp;&amp; appupdate.newVersion" ng-hide="appupdate.msgClose" close="appupdate.msgClose=true" class="warning radius"&gt;&lt;i class="fa fa-info-circle fa-margin"&gt;&lt;/i&gt;Es existiert eine neuere Version {{appupdate.release.version}} vom {{appupdate.release.releasedate | date: "dd.MM.yyyy"}}!&lt;a href="" ng-disabled="!appupdate.releaseChecked" ng-click="appupdate.installRelease()" class="button tiny radius secondary left15 bottom0"&gt;&lt;i class="fa fa-cloud-download fa-margin"&gt;&lt;/i&gt;Jetzt installieren...&lt;/a&gt;&lt;/alert&gt;
              &lt;alert ng-if="appupdate.releaseLoaded &amp;&amp; !appupdate.newVersion" ng-hide="appupdate.msgClose" close="appupdate.msgClose=true" class="info radius"&gt;&lt;i class="fa fa-check fa-margin"&gt;&lt;/i&gt;Sie verfügen bereits über die neueste Version.&lt;/alert&gt;
              &lt;alert ng-if="!appupdate.releaseLoaded" ng-hide="appupdate.msgClose" close="appupdate.msgClose=true" class="error radius"&gt;&lt;i class="fa fa-exclamation-triangle fa-margin"&gt;&lt;/i&gt;Die Releasedaten konnten nicht geladen werden.&lt;/alert&gt;
            &lt;/div&gt;
          &lt;/div&gt;
          &lt;div ng-if="appupdate.newVersion" class="row releaseheader"&gt;
            &lt;div class="small-4 columns"&gt;
              &lt;label&gt;Version&lt;/label&gt;{{appupdate.release.version}}
            &lt;/div&gt;
            &lt;div class="small-4 columns"&gt;
              &lt;label&gt;Freigegeben am&lt;/label&gt;{{appupdate.release.releasedate | date: 'dd.MM.yyyy HH:mm'}}
            &lt;/div&gt;
            &lt;div class="small-4 columns"&gt;
              &lt;label&gt;Autor&lt;/label&gt;{{appupdate.release.author}}
            &lt;/div&gt;
          &lt;/div&gt;
          &lt;div ng-if="appupdate.newVersion" class="row"&gt;
            &lt;div class="small-4 columns"&gt;&lt;strong&gt;Name des Skins&lt;/strong&gt;&lt;/div&gt;
            &lt;div class="small-4 columns"&gt;&lt;strong&gt;Änderungsdatum&lt;/strong&gt;&lt;/div&gt;
            &lt;div class="small-4 columns"&gt;&lt;strong&gt;Aktualisierungsstatus&lt;/strong&gt;&lt;/div&gt;
          &lt;/div&gt;
          &lt;div ng-if="appupdate.newVersion" ng-repeat="skin in appupdate.release.skins" class="row"&gt;
            &lt;div class="small-4 columns"&gt;{{skin.name}}&lt;/div&gt;
            &lt;div class="small-4 columns"&gt;{{skin.update | date: 'dd.MM.yyyy HH:mm'}}&lt;/div&gt;
            &lt;div class="small-4 columns"&gt;{{appupdate.updateStatus(skin.status)}}&lt;i ng-if="skin.status==='updated'" class="fa fa-check-circle fa-lg left10 green"&gt;&lt;/i&gt;&lt;i ng-if="skin.status.substr(0,5)==='error'" class="fa fa-times-circle fa-lg left10 red"&gt;&lt;/i&gt;&lt;/div&gt;
          &lt;/div&gt;
        &lt;/form&gt;
      &lt;/tab&gt;
      &lt;tab heading="Community"&gt;
        &lt;p&gt;custCommunity...&lt;/p&gt;
      &lt;/tab&gt;
    &lt;/tabset&gt;
    &lt;p class="text-center bottom0"&gt;&lt;a ng-click="customize.ok()" class="button success radius tiny"&gt;&lt;i class="fa fa-check fa-margin"&gt;&lt;/i&gt;Einstellungen sichern&lt;/a&gt;&lt;a ng-click="customize.cancel()" class="button alert radius tiny left15"&gt;&lt;i class="fa fa-times fa-margin"&gt;&lt;/i&gt;Änderung abbrechen&lt;/a&gt;&lt;/p&gt;&lt;a ng-click="customize.cancel()" class="close-reveal-modal"&gt;&amp;times;&lt;/a&gt;
  &lt;/div&gt;
&lt;/script&gt;
</content>
</skin>