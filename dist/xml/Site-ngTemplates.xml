                              &lt;button href="" title="Eintrag bearbeiten" ng-click="menuCustom.edit($index)" class="tiny radius secondary square right5"&gt;&lt;i class="fa fa-edit fa-fw"&gt;&lt;/i&gt;&lt;/button&gt;
